{
  "presets": [
    "react"
  ],
  "plugins": [],
  "env": {
    "cjs": {
      "presets": [
        ["es2015", {
          "modules": "commonjs"
        }],
        "react",
        "stage-2"
      ],
      "plugins": [
        "add-module-exports"
      ]
    },
    "es": {
      "presets": [
        ["es2015", {
          "modules": false
        }],
        "react",
        "stage-2"
      ],
      "plugins": [
        "lodash"
      ]
    },
    "test": {
      "presets": [
        "es2015",
        "react",
        "stage-2"
      ],
      "plugins": [
        "add-module-exports"
      ]
    }
  }
}
